<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#670C05"
    tools:context=".MenuAdminActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="383dp"
        android:layout_height="86dp"
        android:layout_marginTop="28dp"
        android:fontFamily="cursive"
        android:gravity="center"
        android:text="Menu Admin"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/menu"
        android:layout_width="164dp"
        android:layout_height="166dp"
        android:layout_marginTop="52dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:srcCompat="@drawable/circulo1" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="85dp"
        android:layout_height="82dp"
        app:layout_constraintBottom_toBottomOf="@+id/menu"
        app:layout_constraintEnd_toEndOf="@+id/menu"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@+id/menu"
        app:layout_constraintTop_toTopOf="@+id/menu"
        app:layout_constraintVertical_bias="0.488"
        app:srcCompat="@drawable/menu_con" />

    <ImageButton
        android:id="@+id/perfilAdmin"
        android:layout_width="164dp"
        android:layout_height="166dp"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:background="#00FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/accionesAdmin"
        app:layout_constraintTop_toBottomOf="@+id/menu"
        app:srcCompat="@drawable/circulo1" />

    <ImageButton
        android:id="@+id/accionesAdmin"
        android:layout_width="164dp"
        android:layout_height="166dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:background="#00FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu"
        app:srcCompat="@drawable/circulo1" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="77dp"
        android:layout_height="76dp"
        app:layout_constraintBottom_toBottomOf="@+id/perfilAdmin"
        app:layout_constraintEnd_toEndOf="@+id/perfilAdmin"
        app:layout_constraintHorizontal_bias="0.54"
        app:layout_constraintStart_toStartOf="@+id/perfilAdmin"
        app:layout_constraintTop_toTopOf="@+id/perfilAdmin"
        app:layout_constraintVertical_bias="0.555"
        app:srcCompat="@drawable/usuario_con" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="82dp"
        android:layout_height="124dp"
        app:layout_constraintBottom_toBottomOf="@+id/accionesAdmin"
        app:layout_constraintEnd_toStartOf="@+id/perfilAdmin"
        app:layout_constraintHorizontal_bias="0.573"
        app:layout_constraintStart_toStartOf="@+id/accionesAdmin"
        app:layout_constraintTop_toTopOf="@+id/accionesAdmin"
        app:srcCompat="@drawable/cosas_admin" />

</androidx.constraintlayout.widget.ConstraintLayout>